{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "chango",
  "version": "0.1.0",
  "identifier": "com.fisica.chango",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Dardo y mono",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/george.png"]
  }
}
